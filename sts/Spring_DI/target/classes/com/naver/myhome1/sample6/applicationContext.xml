<?xml version="1.0" encoding="UTF-8"?>
<!-- 
	해당 패키지에서 New -> Other -> Spring Bean Configuration File -> Next
	-> applicationContext 입력 -> Next -> beans 클릭 -> Finish
 -->
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation=
	"http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean 생성합니다. 
         List타입 매핑의 경우
    -->
	<bean id="CollectionBean" class="com.naver.myhome1.sample6.CollectionBean" >
		<constructor-arg>
			
		</constructor-arg>
	
	   <property name="greeting">
	     <list>
	       <value>서울시 종로구 다동</value>
	       <value>서울시 종로구 인사동</value>
	       <value>서울시 종로구 익선동</value>
	     </list>
	   </property>
	</bean>
    
</beans>